{
  "name": "niyama",
  "version": "1.0.0",
  "description": "My electron app",
  "main": "main.js",
  "scripts": {
    "start": "npx concurrently 'npm run start-electron' 'npm run watch:svelte'",
    "watch:svelte": "cd my-svelte-project && npm run dev",
    "start-electron": "electron-forge start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": [],
  "author": "Keyvan Fatehi",
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "^11.2.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "node-mndp": "git+https://github.com/kfatehi/node-mndp.git",
    "routeros-client": "^1.1.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "niyama"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
